        "z",
        "iconv"
      ],
      "frameworks": [
        "IOKit",
        "Security"
      ],
      "pod_target_xcconfig": {
        "DEFINES_MODULE": "YES",
        "GCC_PREPROCESSOR_DEFINITIONS": "HAVE_INTTYPES_H HAVE_PKCRYPT HAVE_STDINT_H HAVE_WZAES HAVE_ZLIB"
      }
    },
    {
      "name": "ThirdParty",
      "dependencies": {
        "KIF": [

        ]
      },
      "pod_target_xcconfig": {
        "DEFINES_MODULE": "YES",
        "GCC_PREPROCESSOR_DEFINITIONS": "HAVE_INTTYPES_H HAVE_PKCRYPT HAVE_STDINT_H HAVE_WZAES HAVE_ZLIB"
      }
    },
    {
      "name": "LocalThirdParty",
      "public_header_files": "NewMonkey/ThirdParty/**/*.h",
      "source_files": "NewMonkey/ThirdParty/**/*.{h,m,mm,mlmodel,c}",
      "resources": "NewMonkey/ThirdParty/**/*.{xib,storyboard,bundle,js,html}",
      "pod_target_xcconfig": {
        "DEFINES_MODULE": "YES",
        "GCC_PREPROCESSOR_DEFINITIONS": "HAVE_INTTYPES_H HAVE_PKCRYPT HAVE_STDINT_H HAVE_WZAES HAVE_ZLIB"
